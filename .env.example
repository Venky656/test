# Google Gemini API Configuration
# Users will provide their API key through the frontend form
# You can set a default key here for testing, but it's not required
GEMINI_API_KEY=

# Server Configuration
PORT=3000
NODE_ENV=development

# Session Secret for user authentication
SESSION_SECRET=your_secure_session_secret_here

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# PDF Generation
LATEX_TIMEOUT=30000